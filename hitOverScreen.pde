 
   void hitOverScreen() //screen for when obstacle is hit
  {
    background(0,0,0);
  textAlign(CENTER);
  fill(255);
  textSize(width/10);
  text("You hit something!", width/2,height/2.5);
  textSize(width/25);
  text("Game Over", width/2,height/2);
  textSize(width/20);
  text("Click to Restart", width/2,height/1.5);
  
  }
   
